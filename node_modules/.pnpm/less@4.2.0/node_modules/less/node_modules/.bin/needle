#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/needle@3.3.1/node_modules/needle/bin/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/needle@3.3.1/node_modules/needle/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/needle@3.3.1/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules:/Users/fuzhiqiang/Desktop/node_modules:/Users/fuzhiqiang/node_modules:/Users/node_modules:/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less@4.2.0/node_modules/less/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less@4.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/needle@3.3.1/node_modules/needle/bin/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/needle@3.3.1/node_modules/needle/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/needle@3.3.1/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules:/Users/fuzhiqiang/Desktop/node_modules:/Users/fuzhiqiang/node_modules:/Users/node_modules:/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less@4.2.0/node_modules/less/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less@4.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../needle@3.3.1/node_modules/needle/bin/needle" "$@"
else
  exec node  "$basedir/../../../../../needle@3.3.1/node_modules/needle/bin/needle" "$@"
fi
