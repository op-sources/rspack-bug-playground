#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less-plugin-dls@11.12.0/node_modules/less-plugin-dls/bin/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less-plugin-dls@11.12.0/node_modules/less-plugin-dls/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less-plugin-dls@11.12.0/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules:/Users/fuzhiqiang/Desktop/node_modules:/Users/fuzhiqiang/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less-plugin-dls@11.12.0/node_modules/less-plugin-dls/bin/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less-plugin-dls@11.12.0/node_modules/less-plugin-dls/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/less-plugin-dls@11.12.0/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules/.pnpm/node_modules:/Users/fuzhiqiang/Desktop/Micro-Frontends/node_modules:/Users/fuzhiqiang/Desktop/node_modules:/Users/fuzhiqiang/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/dls" "$@"
else
  exec node  "$basedir/../../bin/dls" "$@"
fi
